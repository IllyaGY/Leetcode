class Solution {
public:
    int maxSubArrayLen(vector<int>& nums, int k) {
        int mx = 0;
        long long pref = 0; 
        unordered_map<int, int> mp; 
        mp[0] = -1; 
        for(int i = 0; i < nums.size(); i++){
            pref+=nums[i];
            if(mp.find(pref) == mp.end())mp[pref] = i;
            if(mp.find(pref - k) != mp.end()) mx = max(i - mp[pref-k], mx);  
            
        }
        return mx; 
    }
};
